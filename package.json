{
  "name": "chevrotain-java",
  "version": "0.0.19",
  "description": "java parser based on chevrotain",
  "main": "src/index.js",
  "repository": "url",
  "author": "Thorben von Hacht",
  "license": "MIT",
  "dependencies": {
    "chevrotain": "^3.2.0",
    "xregexp": "^4.0.0"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "eslint": "^4.16.0",
    "eslint-config-google": "^0.9.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-prettier": "^2.5.0",
    "jest": "^22.1.4",
    "jest-cli": "^22.1.4",
    "npm-run-all": "4.1.3",
    "prettier": "1.14.3"
  },
  "scripts": {
    "ci": "npm-run-all build lint format_validate test",
    "build": "node src/gen_diagrams.js",
    "format": "prettier --write \"**/*.@(js|json|md)\"",
    "format_validate": "prettier --list-different \"**/*.@(js|json|md)\"",
    "lint": "eslint src/**/*.js tests/**/*.js",
    "test": "jest"
  },
  "jest": {
    "displayName": "test",
    "snapshotSerializers": [
      "<rootDir>/tests_config/raw-serializer.js"
    ],
    "testRegex": ".*-test\\.js$",
    "collectCoverage": false,
    "transform": {}
  }
}
