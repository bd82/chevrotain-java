{
  "name": "chevrotain-java",
  "version": "0.0.19",
  "description": "java parser based on chevrotain",
  "main": "src/index.js",
  "repository": "url",
  "author": "Thorben von Hacht",
  "license": "MIT",
  "dependencies": {
    "chevrotain": "4.1.0",
    "xregexp": "4.0.0"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "eslint": "5.8.0",
    "eslint-config-google": "0.11.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-import": "2.8.0",
    "mocha": "^5.2.0",
    "npm-run-all": "4.1.3",
    "prettier": "1.14.3"
  },
  "scripts": {
    "ci": "npm-run-all build lint format_validate test",
    "build": "node src/gen_diagrams.js",
    "format": "prettier --write \"**/*.@(js|json|md)\"",
    "format_validate": "prettier --list-different \"**/*.@(js|json|md)\"",
    "lint": "eslint src/**/*.js tests/**/*.js",
    "test": "mocha \"./tests/**/*-test.js\""
  },
  "jest": {
    "displayName": "test",
    "snapshotSerializers": [
      "<rootDir>/tests_config/raw-serializer.js"
    ],
    "testRegex": ".*-test\\.js$",
    "collectCoverage": false,
    "transform": {}
  }
}
